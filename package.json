{
	"name": "taboola-loader",
	"displayName": "taboola-loader",
	"description": "",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.52.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:taboola-loader.taboolaLoader"
	],
	"main": "./out/src/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "taboola-loader.taboolaLoader",
				"title": "Taboola loader"
			}
		],
		"configuration": {
			"title": "Download File Configuration",
			"properties": {
				"download-file.defaultFolder": {
					"type": "string",
					"default": "",
					"description": "Default folder where file should be saved."
				}
			}
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.4",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.52.0",
		"@typescript-eslint/eslint-plugin": "^4.9.0",
		"@typescript-eslint/parser": "^4.9.0",
		"eslint": "^7.15.0",
		"glob": "^7.1.6",
		"mocha": "^8.1.3",
		"pretty-js": "^0.2.1",
		"typescript": "^4.1.2",
		"vscode-test": "^1.4.1"
	},
	"dependencies": {
		"@types/mkdirp": "^1.0.1",
		"js-beautify": "^1.13.0",
		"mkdirp": "^1.0.4"
	}
}
